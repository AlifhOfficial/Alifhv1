{
  "name": "alifh",
  "version": "0.1.0",
  "private": true,
  "description": "Alifh - Vehicle Marketplace & Valuation Platform",
  "packageManager": "bun@1.3.4",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build --filter='!@alifh/mobile'",
    "build:all": "turbo run build",
    "build:web": "turbo run build --filter=@alifh/web",
    "build:mobile": "turbo run build --filter=@alifh/mobile",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "db:seed": "bun scripts/seed.ts",
    "db:reset": "bun scripts/reset-db.ts"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@types/pg": "^8.16.0",
    "postgres": "^3.4.7",
    "turbo": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.10"
  },
  "dependencies": {
    "@neondatabase/serverless": "1.0.2",
    "@paralleldrive/cuid2": "3.0.4",
    "@types/bcrypt": "6.0.0",
    "bcrypt": "6.0.0",
    "dotenv": "17.2.3",
    "drizzle-orm": "0.45.1"
  }
}
